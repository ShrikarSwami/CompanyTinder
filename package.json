{
  "name": "companytinder",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist-electron/electron/main.js",
  "scripts": {
    "dev": "concurrently -n main,renderer,electron -c cyan,green,yellow \"pnpm:dev:main\" \"pnpm:dev:renderer\" \"pnpm:dev:electron\"",
    "dev:main": "tsc -p tsconfig.node.json -w",
    "dev:renderer": "vite",
    "dev:electron": "wait-on tcp:5173 file:dist-electron/electron/main.js && electron .",
    "build": "vite build && tsc -p tsconfig.node.json",
    "rebuild:native": "electron-rebuild -f"
  },
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "dotenv": "^17.2.3",
    "keytar": "^7.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.1",
    "electron": "^38.4.0",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.7.2",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "wait-on": "^9.0.1"
  }
}
