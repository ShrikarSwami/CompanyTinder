{
  "name": "companytinder",
  "version": "0.1.0",
  "private": true,
  "main": "dist-electron/electron/main.js",
  "type": "commonjs",
  "scripts": {
    "dev": "concurrently -k -n main,renderer,electron \"tsc -w -p tsconfig.node.json\" \"vite\" \"wait-on tcp:5173 && electron .\"",
    "dev:main": "tsc -w -p tsconfig.node.json",
    "dev:renderer": "vite",
    "dev:electron": "wait-on tcp:5173 && electron .",
    "build": "rimraf dist-electron && tsc -p tsconfig.node.json && vite build",
    "rebuild:native": "electron-rebuild -f --only better-sqlite3,keytar",
    "start": "electron ."
  },
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "dotenv": "^17.2.3",
    "get-port": "^7.1.0",
    "googleapis": "^164.1.0",
    "keytar": "^7.9.0",
    "open": "^10.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.9.1",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.1",
    "electron": "^38.4.0",
    "electron-builder": "^24.13.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "wait-on": "^9.0.1"
  }
}
